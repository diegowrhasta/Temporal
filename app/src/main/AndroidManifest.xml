        </activity>
        <activity android:name=".Crear_Perfil" />
        <activity
            android:name=".MenuApp"
            android:label="@string/title_activity_menu_app"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>